{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "dist/air-quality-dashboard/browser"
          }
      }
    ],
    "routes": [
      {
        "handle": "filesystem"
      },
      {
        "src": "/.*",
        "dest": "/index.html"
      }
    ]
  }